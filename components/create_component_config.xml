<component name="settings.msg" title="Page groups">
    <dbtable key="_id">msg</dbtable>
    <tablefield>ket,start_time,sts,dt_created</tablefield>
    <headers>
            {Group label    ->  'style'=>'width:40%;text-align:left;'}  
            {Start date     ->  'style'=>'width:25%;text-align:left;'}  
            {STS            ->  'style'=>'width:15%;text-align:left;'}  
            {Created On     ->  'style'=>'width:25%;text-align:left;'}  
    </headers>
    <forms>
        <!-- type only : textfield,number,dropdown,textarea,calendar-->
        <form name="ket" type="textfield">Group label</form> 
        <form name="start_time" type="calendar">Start date</form>  
        <form name="end_time" type="calendar">End date</form>  
        <form name="sts" type="textfield">Sts</form>    
        <form name="msg" type="textarea">Desc</form>    
         
    </forms>
</component>