<component name="texas_groups" title="Texas groups">
    <dbtable key="id">texas_groups</dbtable>
    <tablefield>dealerId,name,tablelow,tablelimit</tablefield>
    <headers>
            {DealerID         ->  'style'=>'width:15%;text-align:left;'}  
            {Group name         ->  'style'=>'width:25%;text-align:left;'}  
            {Table low         ->  'style'=>'width:25%;text-align:left;'}  
            {Table limit         ->  'style'=>'width:10%;text-align:left;'}  
    </headers>
    <forms>
        <!-- type only : textfield,number,dropdown,textarea,calendar-->
        <form name="dealerId" type="textfield">DealerID</form> 
        <form name="name" type="textfield">Group name</form>  
        
        <form name="tablelow" type="textfield">Table low</form>   
        <form name="tablelimit" type="textfield">Table limit</form> 
         
    </forms>
</component>