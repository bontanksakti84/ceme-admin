<component name="players" title="Data players">
    <dbtable key="dealerId">players</dbtable>
    <tablefield>dealerId,username,email,rank,bankName,bankAccount,transferLimit,lastlogin</tablefield>
    <headers>
            {Dealer     ->  'width:20%;','text-align:left;'}
            {Username     ->  'width:15%;','text-align:center;'}
            {Email  ->  'width:15%;','text-align:left;'}
            {Rank  ->  'width:5%;','text-align:left;'}
            {Bank name ->  'width:10%;','text-align:right;'}
            {Bank acc-no ->  'width:10%;','text-align:left;'}
            {Transfer limit ->  'width:15%;','text-align:right;'}
            {Last login ->  'width:15%;','text-align:right;'}
    </headers>
    <forms>
        <!-- type only : textfield,number,dropdown,textarea,calendar-->
        <form name="dealerId" type="textfield">Dealer</form> 
        <form name="username" type="calendar">Username</form> 
        <form name="email" type="textfield">Email</form> 
        <form name="rank" type="textfield">Rangking</form> 
        
        <form name="bankName"  type="textfield">Bank name</form> 
        <form name="bankAccount" type="textfield">Account number</form> 
        <form name="transferLimit" type="number">Transfer limit </form>  
    </forms>
</component>